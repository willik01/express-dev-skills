<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Express - skills</title>
</head>
<body>
    <h1>Skills</h1>
    <ul>
      <% skills.forEach(function(s) { %>
        <li>
          <%= s.id %>
            - 
          <%= s.skill %>
            -
          <%= s.expertise %>
          <!-- <%= s.done ? 'done' : 'not done' %> -->
        </li>
      <% }); %>
    </ul>
</body>
</html>
